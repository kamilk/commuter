<!DOCTYPE html>
<html>
<head>
  <title>Commuter</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render 'layouts/navbar' %>

  <% unless notice.blank? %>
    <p class="notice"><%= notice %></p>
  <% end %>
  <% unless alert.blank? %>
    <p class="alert"><%= alert %></p>
  <% end %>

  <%= yield %>

</body>
</html>
